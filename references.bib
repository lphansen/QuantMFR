@article{MiaoRivera:2016,
 abstract = {We study a continuous-time contracting problem under hidden action,$\backslash$nwhere the principal has ambiguous beliefs about the project cash flows.$\backslash$nThe principal designs a robust contract that maximizes his utility under$\backslash$nthe worst-case scenario subject to the agent's incentive and$\backslash$nparticipation constraints. Robustness generates endogenous belief$\backslash$nheterogeneity and induces a tradeoff between incentives and ambiguity$\backslash$nsharing so that the incentive constraint does not always bind. We$\backslash$nimplement the optimal contract by cash reserves, debt, and equity. In$\backslash$naddition to receiving ordinary dividends when cash reserves reach a$\backslash$nthreshold, outside equity holders also receive special dividends or$\backslash$ninject cash in the cash reserves to hedge against model uncertainty and$\backslash$nsmooth dividends. The equity premium and the credit yield spread$\backslash$ngenerated by ambiguity aversion are state dependent and high for$\backslash$ndistressed firms with low cash reserves.},
