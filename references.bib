@article{HansenSargent:2010,
 abstract = {A representative consumer uses Bayes' law to learn about parameters of several models and to construct probabilities with which to perform ongoing model averaging. The arrival of signals induces the consumer to alter his posterior distribution over models and parameters. The consumer's specification doubts induce him to slant probabilities pessimistically. The pessimistic probabilities tilt toward a model that puts long-run risks into consumption growth. That contributes a countercyclical history-dependent component to prices of risk. {\textcopyright} 2010 Lars Peter Hansen and Thomas J. Sargent.},
 author = {Hansen, Lars Peter and Sargent, Thomas J.},
