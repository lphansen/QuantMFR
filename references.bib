@article{RyderHeal:1973,
 abstract = {In the literature on optimal economic growth, considerable effort has been devoted to the analysis of models using a preference function which depends additively on consumption at the various dates within the planning period. The typical formulation is that used by Cass: J[c({\textperiodcentered})] = {\&}integ;08 e-dtu[c(t)]dt, {\ldots}(1) where d = 0 is an exponential discount factor, c(t) is the current consumption level at date t. Koopmans has shown that this formulation of the preference function is implied by certain assumptions of existence, continuity, sensitivity, stationarity, boundedness and independence. Hicks has identified independence as the key assumption, arguing that it is counter-intuitive. Instead, he claims that there is normally a strong complementarity between consumption at successive moments. This view is widely held, but seldom practiced "because we do not know how to specify [complementarities] in an analytical manner which would be deemed adequate to the problem". The object of this paper is to investigate the effect of using a model of the sources of consumer satisfaction that is possibly more realistic than the usual formulation of equation (1). The essential point is that it introduces into the utility function a new variable, z, which may be interpreted either as the customary level of consumption, or as the expected level of consumption. Instantaneous satisfaction then depends both on instantaneous consumption and on the customary or expected consumption level. [ABSTRACT FROM AUTHOR]},
 author = {Ryder, Harl E. and Heal, Geoffrey M.},
